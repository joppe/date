        "@typescript-eslint/eslint-plugin": "^4.28.4",
        "@typescript-eslint/parser": "^4.28.4",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "import-sort-style-module": "^6.0.0",
        "jest": "^27.0.6",
        "pettier": "^1.1.0",
        "prettier": "^2.3.2",
